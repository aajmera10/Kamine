            <com.example.xina.kamine.Utils.PoppinsRegular
                android:id="@+id/textView102"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="24dp"
                android:text="X Design Essentials Red BAsic Design For Now "
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/imageView24"
                app:layout_constraintTop_toTopOf="@+id/imageView24" />

            <com.example.xina.kamine.Utils.PoppinsRegular
                android:id="@+id/textView107"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="8dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="4dp"
                android:text="RS 577"
                android:textStyle="bold"
                app:layout_constraintStart_toEndOf="@+id/imageView24"
                app:layout_constraintTop_toBottomOf="@+id/textView102" />

            <com.example.xina.kamine.Utils.PoppinsRegular
                android:id="@+id/textView173"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="8dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="4dp"
                android:text="Color"
                android:textStyle="bold"
                app:layout_constraintStart_toEndOf="@+id/imageView24"
                app:layout_constraintTop_toBottomOf="@+id/textView107" />

            <com.example.xina.kamine.Utils.PoppinsRegular
                android:id="@+id/textView174"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50dp"
                android:layout_marginStart="50dp"
                android:text="Size"
                app:layout_constraintBottom_toBottomOf="@+id/textView173"
                app:layout_constraintStart_toEndOf="@+id/textView173"
                app:layout_constraintTop_toTopOf="@+id/textView173" />

            <com.example.xina.kamine.Utils.PoppinsRegular
                android:id="@+id/textView175"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_marginStart="8dp"
                android:text="Quantity"
                app:layout_constraintBaseline_toBaselineOf="@+id/textView174"
                app:layout_constraintEnd_toEndOf="@+id/textView102"
                app:layout_constraintStart_toEndOf="@+id/textView174" />

            <com.example.xina.kamine.Utils.PoppinsRegular
                android:id="@+id/textView176"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="8dp"
                android:layout_marginRight="8dp"
                android:layout_marginTop="10dp"
                android:text="Multicolor"
                app:layout_constraintEnd_toStartOf="@+id/textView174"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/textView173"
                app:layout_constraintTop_toBottomOf="@+id/textView173" />

            <com.example.xina.kamine.Utils.PoppinsRegular
                android:id="@+id/poppinsRegular"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_marginStart="8dp"
                android:text="Xl"
                app:layout_constraintBaseline_toBaselineOf="@+id/textView176"
                app:layout_constraintEnd_toEndOf="@+id/textView174"
                app:layout_constraintStart_toStartOf="@+id/textView174" />

            <com.example.xina.kamine.Utils.PoppinsRegular
                android:id="@+id/poppinsRegular2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_marginStart="8dp"
                android:text="1"
                app:layout_constraintBottom_toBottomOf="@+id/poppinsRegular"
                app:layout_constraintEnd_toEndOf="@+id/textView175"
                app:layout_constraintStart_toStartOf="@+id/textView175"
                app:layout_constraintTop_toTopOf="@+id/poppinsRegular" />

            <com.example.xina.kamine.Utils.PoppinsMedium
                android:id="@+id/textView177"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="Quantity To Return"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/view14" />

            <TextView
                android:id="@+id/textView178"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="15dp"
                android:text="TextView"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView177" />

            <Spinner
                android:id="@+id/spinner6"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="16dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="8dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView183" />
        </android.support.constraint.ConstraintLayout>
    </android.support.v4.widget.NestedScrollView>
</android.support.constraint.ConstraintLayout>